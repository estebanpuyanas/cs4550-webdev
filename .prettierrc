{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "yaml": {
    "useTabs": false,
    "tabWidth": 2
  },
  "semi": true,
  "singleQuote": true,
  "quoteProps": "consistent",
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "lf"
}
